{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 9 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        {
            "type" : "package",
            "package" : {
                "name" : "hcpss/structure_sync",
                "version" : "1.0",
                "type" : "drupal-module",
                "source" : {
                    "url" : "https://github.com/hcpss-banderson/structure_sync.git",
                    "type" : "git",
                    "reference" : "8.x-1.x"
                }
            }
        },
        {
            "type" : "package",
            "package" : {
                "name" : "hcpss/parity",
                "version" : "dev-master",
                "type" : "drupal-theme",
                "source" : {
                    "url" : "https://github.com/HCPSS/parity.git",
                    "type" : "git",
                    "reference" : "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "hcpss/hcpss_school_vocabulary",
                "version": "dev-master",
                "type": "drupal-module",
                "source": {
                    "url": "https://github.com/HCPSS/hcpss_school_vocabulary.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.6",
        "drupal/config_split": "^1.5",
        "drupal/console": "^1.0.2",
        "drupal/core-composer-scaffold": "^9",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^9",
        "drupal/devel": "^4.0",
        "drupal/disable_messages": "^2.0",
        "drupal/field_group": "^3.1",
        "drupal/honeypot": "^2.0",
        "drupal/node_view_permissions": "^1.3",
        "drupal/paragraphs": "^1.12",
        "drupal/simple_oauth": "^5.0",
        "drush/drush": "^10.3",
        "hcpss/hcpss_school_vocabulary": "dev-master",
        "hcpss/parity": "dev-master",
        "hcpss/structure_sync": "^1.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
